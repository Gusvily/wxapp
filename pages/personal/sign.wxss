/* pages/personal/sign.wxss */

page {
  background: #302f34;
  height: 100%;
}

.wrap {
  padding-top: 100px;
}

.content {
  width: 650rpx;
  height: 780rpx;
  background: url('data:image/png;base64,');
  background-size: cover;
  margin: 0 auto;
  padding: 100rpx 56rpx 70rpx;
  position: relative;
}

.qdwc {
  width: 118rpx;
  height: 118rpx;
  display: block;
  margin: 0 auto;
}

.title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
  text-align: center;
  margin-bottom: 20rpx;
}

.time {
  font-size: 24rpx;
  color: #333;
  text-align: center;
  margin: 20rpx auto 0;
}

.label {
  color: #31375a;
  font-size: 24rpx;
  font-weight: bold;
  margin-top: 30rpx;
  margin-bottom: 28rpx;
}

.tips {
  color: #31375a;
  font-size: 24rpx;
  overflow-y: scroll;
  height: 300rpx;
}

.button {
  width: 460rpx;
  margin: 0 auto;
  height: 74rpx;
  line-height: 74rpx;
  text-align: center;
  border-radius: 37rpx;
  color: #fff;
  margin-top: 40rpx;
}

.msg {
  margin: 100rpx;
  height: 74rpx;
  line-height: 74rpx;
  color: #fff;
  text-align: center;
  font-size: 32rpx;
}

.msgTip {
  background: #7f7f7f;
  display: inline-block;
  border-radius: 12rpx;
  padding: 0 20rpx;
}

button[disabled]:not([type]) {
  color: #fff;
}
